	echo "%%% NOTE %%%  Gtool5 will be built for MPI programs. "
	echo "              If features for MPI are not needed, rerun ./configure with \"--disable-mpi\" option "
fi

echo ""
echo "  Execute GNU make in the current directory, as follows."
echo ""
echo "    $MAKE"
echo ""
