		{ { echo "$as_me:$LINENO: error: specified library file \"$ac_cv_lib_netcdf\" have invalid suffix. Valid suffixes are \".a\" or \".so\"" >&5
echo "$as_me: error: specified library file \"$ac_cv_lib_netcdf\" have invalid suffix. Valid suffixes are \".a\" or \".so\"" >&2;}
   { (exit 1); exit 1; }; }
		;;
	esac

	if test -n "$ac_cv_lib_netcdff"; then

	if test ! -f $ac_cv_lib_netcdff ; then
		{ { echo "$as_me:$LINENO: error: specified library file \"$ac_cv_lib_netcdff\" is not exist" >&5
echo "$as_me: error: specified library file \"$ac_cv_lib_netcdff\" is not exist" >&2;}
   { (exit 1); exit 1; }; }
	fi
	netcdff_libdir=`dirname $ac_cv_lib_netcdff`
	case "$ac_cv_lib_netcdff" in
	    *.a)
		netcdff_libname=`basename $ac_cv_lib_netcdff .a | sed 's/^lib//'`
		;;
	    *.so)
		netcdff_libname=`basename $ac_cv_lib_netcdff .so | sed 's/^lib//'`
		;;
	    *)
		{ { echo "$as_me:$LINENO: error: specified library file \"$ac_cv_lib_netcdff\" have invalid suffix. Valid suffixes are \".a\" or \".so\"" >&5
echo "$as_me: error: specified library file \"$ac_cv_lib_netcdff\" have invalid suffix. Valid suffixes are \".a\" or \".so\"" >&2;}
   { (exit 1); exit 1; }; }
		;;
	esac

	fi

	if test -n "$ac_cv_netcdf_include" && test -f "$ac_cv_netcdf_include"; then
	    netcdf_incdir=`dirname $ac_cv_netcdf_include`
	elif test -n "$ac_cv_netcdf_include" && test -d "$ac_cv_netcdf_include"; then
	    netcdf_incdir=$ac_cv_netcdf_include
	else
	    try2=`dirname $netcdf_libdir`/include
	    try2="$try2 `echo $netcdf_libdir | sed -e 's|/lib/|/include/|' -e 's|/lib$|/include|'`"
	    for try3 in $try2; do
	        if test -f "$try3/netcdf.h"; then
		    netcdf_incdir=$try3
		    break
	        fi
	    done
	fi

	test -d "$netcdf_incdir" || { { echo "$as_me:$LINENO: error: netcdf include file not found. specify netcdf include filename (like /usr/lib/netcdf.h) with --with-netcdf-include=" >&5
echo "$as_me: error: netcdf include file not found. specify netcdf include filename (like /usr/lib/netcdf.h) with --with-netcdf-include=" >&2;}
   { (exit 1); exit 1; }; }

	#=== mpiexec, mpirun
	#
	default_bin_mpiexec=no
	ac_cv_bin_mpiexec=${default_bin_mpiexec}


# Check whether --with-mpiexec was given.
if test "${with_mpiexec+set}" = set; then
  withval=$with_mpiexec;
		ac_cv_bin_mpiexec=$withval

else

		{ echo "$as_me:$LINENO: checking mpi executable command (like mpiexec, mpirun)" >&5
echo $ECHO_N "checking mpi executable command (like mpiexec, mpirun)... $ECHO_C" >&6; }
if test "${ac_cv_bin_mpiexec+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  { { echo "$as_me:$LINENO: error: specify mpi executable command (like mpiexec, mpirun) with --with-mpiexec=" >&5
echo "$as_me: error: specify mpi executable command (like mpiexec, mpirun) with --with-mpiexec=" >&2;}
   { (exit 1); exit 1; }; }

fi
{ echo "$as_me:$LINENO: result: $ac_cv_bin_mpiexec" >&5
echo "${ECHO_T}$ac_cv_bin_mpiexec" >&6; }

fi



	case $ac_cv_bin_mpiexec in
	yes)
	    { { echo "$as_me:$LINENO: error: specify mpi executable command (like mpiexec, mpirun) with --with-mpiexec=" >&5
echo "$as_me: error: specify mpi executable command (like mpiexec, mpirun) with --with-mpiexec=" >&2;}
   { (exit 1); exit 1; }; }
	    ;;
	no)
	    MPIEXEC=
	    MPIEXECPROCOPT=
	    MPIEXECNODEOPT=
	    ;;
	*)
	    if test ! -f "${ac_cv_bin_mpiexec}" ; then
		ac_cv_bin_mpiexec_full=`which ${ac_cv_bin_mpiexec}`
		if test -z "${ac_cv_bin_mpiexec_full}" ; then
		    { { echo "$as_me:$LINENO: error: \"${ac_cv_bin_mpiexec}\" command is not found" >&5
echo "$as_me: error: \"${ac_cv_bin_mpiexec}\" command is not found" >&2;}
   { (exit 1); exit 1; }; }
		fi
	    fi

	    case "${CPPFLAGS}" in
		*"-DLIB_MPI"*) ;;
		*)
		    CPPFLAGS="${CPPFLAGS} -DLIB_MPI";;
	    esac

	    case $ac_cv_bin_mpiexec in
	    *mpiexec*)
		MPIEXEC=$ac_cv_bin_mpiexec
		MPIEXECPROCOPT=-n
		MPIEXECNODEOPT=-N
		;;
	    *mpirun*)
		MPIEXEC=$ac_cv_bin_mpiexec
		MPIEXECPROCOPT=-np
		MPIEXECNODEOPT=-n
		;;
	    *)
		MPIEXEC=$ac_cv_bin_mpiexec
		MPIEXECPROCOPT=
		MPIEXECNODEOPT=
		echo 'WARNING: "$ac_cv_bin_mpiexec" is unknown command. So edit "MPIEXECNODEOPT" and "MPIEXECPROCOPT" in Config.mk afterward.'
	        ;;
	    esac

	    mpi_autosetting=no

	    ;;
	esac






	if test -n "${MPIEXEC}" ; then
	    if test -z "${MPIFC}" ; then
	        { { echo "$as_me:$LINENO: error: \"${FC}\" compiler does not support MPI. Specify MPI fortran compiler to \"\$FC\"" >&5
echo "$as_me: error: \"${FC}\" compiler does not support MPI. Specify MPI fortran compiler to \"\$FC\"" >&2;}
   { (exit 1); exit 1; }; }
	    fi
	    ac_cv_gt5suffix=mpi
	fi

############################################################
