#

	mpifc_ldlibs=
	mpifc_ldflags=
	mpifc_fflags=

	if test -n "${MPIEXEC}" ; then
	    #
	    #== for mpich
	    #
	    #for compopt in `${MPIFC} -compile-info`; do
	    for compopt in `${MPIFC} -show`; do
		case $compopt in
		-I)	;;
		"-I"*)
		mpifc_fflags="$mpifc_fflags $compopt" ;;
		"-L"*)
		mpifc_ldflags="$mpifc_ldflags $compopt" ;;
		"-l"*)
		mpifc_ldlibs="$mpifc_ldlibs $compopt" ;;
		-s)	;;
		-c)	;;
		-f)	;;
		"-"*)
		mpifc_fflags="$mpifc_fflags $compopt" ;;
		*)	;;
		esac
	    done
	fi


